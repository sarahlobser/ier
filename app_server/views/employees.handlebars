{{employee.title}} {{employee.name}} {{employee.email}} {{employee.bio}}

<ul>
    {{#each employees}}
    <li>
        <!--
        {{this.title}}
        {{this.name}}
        {{this.email}}
        {{this.bio}}
-->

        <figure class="snip0057 red">
            <figcaption>
                <h2>{{this.name}}</h2>
                <p>{{this.bio}}</p>
                <div class="icons"><a href="#"><i class="ion-ios-home"></i></a><a href="#"><i class="ion-ios-email"></i></a><a href="#"><i class="ion-ios-telephone"></i></a></div>
            </figcaption>
            <div class="image"><img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/331810/sq-sample4.jpg" alt="sample4" /></div>
            <div class="position">{{this.title}}</div>
        </figure>
    </li>
    {{/each}}
</ul>