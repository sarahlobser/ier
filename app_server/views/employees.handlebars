<!-- Introduction Row -->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Our Team
                </h1>
        <p>I bet you never thought you'd meet a group of people who are just as INTENSE as you are! Well prepare 
        to be surprised...</p>
    </div>
</div>

<div class="row">
    <div class="col-lg-12">
        <h2 class="page-header">Our Team</h2>
    </div>

    {{#each employees}}
    <div class="col-lg-4 col-sm-6 text-center empBlock">
        <img class="img img-responsive img-center" src="/images/{{this.name}}.jpg" width="250" alt="">
        <h3>{{this.name}}
                    <small>{{this.title}}</small>
                </h3>
        <p class="bio">{{this.bio}}</p>
    </div>
    {{/each}}
</div>

<hr>

<!-- Footer -->
<footer>
    <div class="row">
        <div class="col-lg-12">
            <p>Copyright &copy; IER 2014</p>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
</footer>
</div>

<!-- jQuery -->
<script src="/stylesheets/startbootstrap-round-about-1.0.4/js/jquery.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src="/stylesheets/startbootstrap-round-about-1.0.4/js/bootstrap.min.js"></script>